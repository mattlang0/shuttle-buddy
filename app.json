{
  "expo": {
    "name": "shuttle-buddy",
    "slug": "shuttle-buddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/squareIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF6E25"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mlang1.shuttlebuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/squareIconTransparent.png",
        "backgroundColor": "#FF6E25"
      },
      "package": "com.mlang1.shuttlebuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc717e4a-61b0-4cd2-9c38-27952e456281"
      }
    }
  }
}
